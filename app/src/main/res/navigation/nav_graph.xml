<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.imoteb.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_before_TestMezajQuestionFragment"
            app:destination="@id/before_TestMezajQuestionFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_mainFragment_to_mezajRecordsFragment"
            app:destination="@id/mezajRecordsFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_mainFragment_to_damMezajDetailFragment"
            app:destination="@id/damMezajDetailFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_mainFragment_to_safraMezajDetailFragment"
            app:destination="@id/safraMezajDetailFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_mainFragment_to_balghamMezajDetailFragment"
            app:destination="@id/balghamMezajDetailFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_mainFragment_to_sodaMezajDetailFragment"
            app:destination="@id/sodaMezajDetailFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
    </fragment>

    <fragment
        android:id="@+id/before_TestMezajQuestionFragment"
        android:name="com.example.imoteb.MezajsTest.Before_TestMezajQuestionFragment"
        android:label="fragment_before__test_mezaj_question"
        tools:layout="@layout/fragment_before__test_mezaj_question" >
        <action
            android:id="@+id/action_before_TestMezajQuestionFragment_to_mezaj_QueriesFragment"
            app:destination="@id/mezaj_QueriesFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_before_TestMezajQuestionFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>

    <fragment
        android:id="@+id/test_mezaj_resultFragment"
        android:name="com.example.imoteb.MezajsTest.Test_mezaj_resultFragment"
        android:label="fragment_test_mezaj_result"
        tools:layout="@layout/fragment_test_mezaj_result" />

    <fragment
        android:id="@+id/mezaj_QueriesFragment"
        android:name="com.example.imoteb.MezajsTest.Mezaj_QueriesFragment"
        android:label="fragment_mezaj_queries"
        tools:layout="@layout/fragment_mezaj_queries" >
        <action
            android:id="@+id/action_mezaj_QueriesFragment_to_test_mezaj_resultFragment"
            app:destination="@id/test_mezaj_resultFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_mezaj_QueriesFragment_to_before_TestMezajQuestionFragment"
            app:destination="@id/before_TestMezajQuestionFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/mezajRecordsFragment"
        android:name="com.example.imoteb.MezajsTest.MezajRecordsFragment"
        android:label="fragment_mezaj_records"
        tools:layout="@layout/fragment_mezaj_records" >
        <action
            android:id="@+id/action_mezajRecordsFragment_to_test_mezaj_resultFragment"
            app:destination="@id/test_mezaj_resultFragment"
            app:enterAnim="@anim/from_left"
            app:exitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_mezajRecordsFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/damMezajDetailFragment"
        android:name="com.example.imoteb.MezajsTest.DamMezajDetailFragment"
        android:label="fragment_dam_mezaj_detail"
        tools:layout="@layout/fragment_dam_mezaj_detail" >
        <action
            android:id="@+id/action_damMezajDetailFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/balghamMezajDetailFragment"
        android:name="com.example.imoteb.MezajsTest.BalghamMezajDetailFragment"
        android:label="fragment_balgham_mezaj_detail"
        tools:layout="@layout/fragment_balgham_mezaj_detail" >
        <action
            android:id="@+id/action_balghamMezajDetailFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/safraMezajDetailFragment"
        android:name="com.example.imoteb.MezajsTest.SafraMezajDetailFragment"
        android:label="fragment_safra_mezaj_detail"
        tools:layout="@layout/fragment_safra_mezaj_detail" >
        <action
            android:id="@+id/action_safraMezajDetailFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>
    <fragment
        android:id="@+id/sodaMezajDetailFragment"
        android:name="com.example.imoteb.MezajsTest.SodaMezajDetailFragment"
        android:label="fragment_soda_mezaj_detail"
        tools:layout="@layout/fragment_soda_mezaj_detail" >
        <action
            android:id="@+id/action_sodaMezajDetailFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left" />
    </fragment>

</navigation>